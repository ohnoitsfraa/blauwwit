{
  "manifest_version": 3,
  "name": "Blauwwit Negeer",
  "description": "Blauwwit.be negeerlijst extensie",
  "version": "1.0",
  "action": {
    "default_popup": "extension.html",
    "default_icon": "extension.png"
  },
  "icons": {
    "16": "extension.png",
    "48": "extension.png",
    "128": "extension.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.blauwwit.be/*"],
      "js": ["jquery.js", "contextscript.js"]
    }
  ]
}
